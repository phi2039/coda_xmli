<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:acom="http://www.coda.com/eassets/schemas/common" targetNamespace="http://www.coda.com/eassets/schemas/common" elementFormDefault="qualified">

  <xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/common/types.xsd"/>

  <xsd:simpleType name="typeBook">
    <xsd:annotation>
      <xsd:documentation>Specifies whether this book master is the primary (posting) book, a secondary book or a currency book.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="com:typeBaseEnum">
      <xsd:enumeration value="ast_booktype_primary">
        <xsd:annotation>
          <xsd:documentation>Indicates that this book master is the primary (posting) book.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ast_booktype_secondary">
        <xsd:annotation>
          <xsd:documentation>Indicates that this book master is a secondary book.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ast_booktype_currency">
        <xsd:annotation>
          <xsd:documentation>Indicates that this book master is a currency book.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBrowseMultieditUpdateAllowRevaluation">
    <xsd:annotation>
      <xsd:documentation>Specifies whether revaluation is enabled, disabled or unchanged for the selected assets during a multi-edit.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="com:typeBaseEnum">
      <xsd:enumeration value="ast_browse_multiedit_leave_reval">
        <xsd:annotation>
          <xsd:documentation>Leaves the current setting for each individual asset unchanged.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ast_browse_multiedit_disable_reval">
        <xsd:annotation>
          <xsd:documentation>Prevents all the selected assets from being revaluation.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ast_browse_multiedit_enable_reval">
        <xsd:annotation>
          <xsd:documentation>Allows all the selected assets to be revalued.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBrowseMultieditUpdateComments">
    <xsd:annotation>
      <xsd:documentation>Specifies how comments are handled when multi-editing assets from Browse.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="com:typeBaseEnum">
      <xsd:enumeration value="ast_browse_multiedit_append_comment">
        <xsd:annotation>
          <xsd:documentation>The new comments are appended to each asset's existing comments.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ast_browse_multiedit_replace_comment">
        <xsd:annotation>
          <xsd:documentation>The new comments replace each asset's existing comments.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeCreation">
    <xsd:annotation>
      <xsd:documentation>Not used.</xsd:documentation>
    </xsd:annotation>
