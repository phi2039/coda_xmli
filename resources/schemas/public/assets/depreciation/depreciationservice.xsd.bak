      <xsd:documentation>Cancels the review of the specified manual depreciation process.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="dep:CancelReviewedManualDepreciationRequest" maxOccurs="unbounded"/>
