        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="BookMasterRequest">
    <xsd:annotation>
      <xsd:documentation>This service allows you to maintain book masters.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ServiceRequest">
        <xsd:all>
          <xsd:element name="Add" type="bkms:AddRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Creates new book masters in the database using the information you provide.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Update" type="bkms:UpdateRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Updates book masters in the database using the information you provide.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Get" type="bkms:GetRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves the specified book masters from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Delete" type="bkms:DeleteRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Deletes the specified book masters from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="List" type="bkms:ListRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves key information for the specified book masters from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetPostingBook" type="bkms:GetPostingBookRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves the code and company code of the posting book from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ListAssociationBooks" type="bkms:ListRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Lists non-currency books associated with the book masters selected by the filter.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ListCurrencyBooks" type="bkms:ListRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Lists the currency books of the book masters selected by the filter.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:all>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="BookMasterResponse">
    <xsd:annotation>
      <xsd:documentation>This is the response that is returned when you maintain book masters.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ServiceResponse">
        <xsd:all>
          <xsd:element name="Add" type="bkms:AddResponseVerb" minOccurs="0">
