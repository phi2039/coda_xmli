      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="GetPostingBookRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Retrieves the code and company code of the posting book from the database.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="bkms:GetPostingBookRequest" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="GetPostingBookResponse">
    <xsd:complexContent>
      <xsd:extension base="com:Response">
