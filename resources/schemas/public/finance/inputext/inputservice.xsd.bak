  <xsd:include schemaLocation="s:/schemas/public/finance/inputext/input.xsd"/>

  <xsd:complexType name="SelectTemplateRequest">
    <xsd:complexContent>
      <xsd:extension base="com:Request">
        <xsd:sequence>
          <xsd:element name="Key" type="com:Key">
            <xsd:annotation>
              <xsd:documentation>This element is a key consisting of a company code and an input template master code.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DocCode" type="com:typeGeneralCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The code of the document master you wish to use. This is only relevant if the specified input template master is linked to more than one document master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SelectTemplateRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Retrieves the specified input template master.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="inx:SelectTemplateRequest" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SelectTemplateResponse">
    <xsd:complexContent>
      <xsd:extension base="com:Response">
        <xsd:sequence>
          <xsd:element name="InputTemplate" type="itm:InputTemplate" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the input template master you specified in the request corresponding to this response.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DocumentMaster" type="doc:DocumentMaster" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the document master linked to the specified input template master. If the input template master is linked to multiple document masters, the document masters are returned in DocCodeList. You should repeat the SelectTemplateRequest, supplying the code of the document master you wish to use.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Transaction" type="txn:Transaction" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the default transaction details as determined by the input template master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AddedLine" type="txn:Line" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the default additional line values as determined by the input template master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PostData" type="inx:PostData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains details that are used when the document is posted, or when checking that the document is postable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DocCodeList" type="itm:DocCodeList" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>When the specified input template master is linked to multiple document masters, this element lists the codes of the linked document masters. If this list is returned, you should repeat the SelectTemplateRequest, supplying the code of the document master you wish to use.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SelectTemplateResponseVerb">
    <xsd:annotation>
      <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ResponseVerb">
        <xsd:sequence>
          <xsd:element name="Response" type="inx:SelectTemplateResponse" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SetHeaderRequest">
    <xsd:complexContent>
      <xsd:extension base="com:Request">
        <xsd:sequence>
          <xsd:element name="NewHeader" type="txn:Header">
            <xsd:annotation>
              <xsd:documentation>Contains the new document header information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewDocWideData" type="inx:DocumentWideData">
            <xsd:annotation>
              <xsd:documentation>Contains the new document-wide data consisting of due date, value date and external references.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PostData" type="inx:PostData">
            <xsd:annotation>
              <xsd:documentation>Contains details that are used when the document is posted, or when checking that the document is postable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Transaction" type="txn:Transaction">
            <xsd:annotation>
              <xsd:documentation>Contains the transaction details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SetHeaderRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Changes the header information on a document.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="inx:SetHeaderRequest" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SetHeaderResponse">
    <xsd:complexContent>
      <xsd:extension base="com:Response">
        <xsd:sequence>
          <xsd:element name="Transaction" type="txn:Transaction" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the transaction details, with the new header information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PostData" type="inx:PostData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains details that are used when the document is posted, or when checking that the document is postable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NumberOfDecimals" type="com:typeDecPlace" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The number of decimal places in the document currency.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SelectedValue" type="com:typeMoneyB" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The SelectedValue supplied in the request corresponding to this response.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SetHeaderResponseVerb">
    <xsd:annotation>
      <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:Response">
        <xsd:sequence>
          <xsd:element name="Response" type="inx:SetHeaderResponse" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="CheckPostRequest">
    <xsd:complexContent>
      <xsd:extension base="com:Request">
        <xsd:sequence>
          <xsd:element name="Transaction" type="txn:Transaction">
            <xsd:annotation>
              <xsd:documentation>Contains the transaction details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PostData" type="inx:PostData">
            <xsd:annotation>
              <xsd:documentation>Contains details that are used when the document is posted, or when checking that the document is postable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SelectedValue" type="com:typeMoneyB" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains a value selected from the database using the selector defined on the input template and is in the currency defined on the input template. Other fields may be derived from the SelectedValue which is why it should be supplied.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="CheckPostRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Checks whether the specified document is valid for posting to the Books or the Intray.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="inx:CheckPostRequest" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="postto" type="com:typeCtDocPost" use="required">
          <xsd:annotation>
            <xsd:documentation>Specify whether the document should be checked for posting to the Books or the Intray.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ignoresurplusdata" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Instructs the server to ignore certain irrelevant data, such as 1099 codes on lines that do not have an account that uses 1099. When TRUE, the server ignores data in the following structures: ExtraPayData, ExtraQty, ExtraTax, ExtraTen99 and ExtraDiscount. When FALSE, the server populates these structures and returns them to the client.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="CheckPostResponse">
    <xsd:complexContent>
      <xsd:extension base="com:Response">
        <xsd:sequence>
          <xsd:element name="Problems" type="inx:Problems" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains details of any problems that invalidate the specified document for posting to the Books or the Intray.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="CheckPostResponseVerb">
    <xsd:annotation>
      <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:Response">
        <xsd:sequence>
          <xsd:element name="Response" type="inx:CheckPostResponse" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

