  <xsd:complexType name="InputRequest">
    <xsd:annotation>
      <xsd:documentation>This service allows you to post financial documents to the Books or the Intray.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ServiceRequest">
        <xsd:all>
          <xsd:element name="SelectTemplate" type="inx:SelectTemplateRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves the specified input template master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SetHeader" type="inx:SetHeaderRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Changes the header information on a document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CheckPost" type="inx:CheckPostRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Checks whether the specified document is valid for posting to the Books or the Intray.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CheckPostWithoutTemplate" type="inx:CheckPostWithoutTemplateRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>As per CheckPost with the exception that the document does not need to have a Financials input template.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Post" type="inx:PostRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Posts the specified document to the Books or the Intray.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PostToIntray" type="inx:PostToXRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Posts the specified document to the Intray.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PostToBooks" type="inx:PostToXRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Posts the specified document to the Books.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CalcCurrencies" type="inx:CalcCurrRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Calculates the value of a specified line following currency edits.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="RefreshTotals" type="inx:RefreshTotalsRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Calculates the values to be displayed in the footer fields based on the current information on the document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SelectIntray" type="inx:SelectIntrayRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Selects documents on the Intray, using the specified filter.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IntrayAutoPost" type="inx:IntrayAutoPostRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Posts the specified documents on the Intray, to the Books.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IntrayDelete" type="inx:IntrayDeleteRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Deletes the specified document(s) on the Intray.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IntrayLoad" type="inx:IntrayLoadRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Loads the specified document on the Intray.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IntrayUnAuthorise" type="inx:IntrayUnAuthoriseRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Removes authorisation from a document that has completed its associated workflow.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GenerateTax" type="inx:GenerateTaxRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Generates tax lines for the specified document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GenerateTaxWithoutTemplate" type="inx:GenerateTaxWithoutTemplateRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>As per GenerateTax with the exception that the document does not need to have a Financials input template.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AutoBalanceTax" type="inx:AutoBalanceTaxRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Balances the specified document by editing the generated tax line to bring the document into balance in document currency.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CancelDoc" type="inx:CancelDocRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Posts cancelling documents to cancel the specified document(s) in the Books.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GenerateBalancingLines" type="inx:GenerateBalancingLinesRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Generates balancing lines on the specified document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BalancingReview" type="inx:BalancingReviewRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Returns the sum of values on the specified document for each balancing element and the whole document (in document, home and dual currencies).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CurrReview" type="inx:CurrReviewRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Returns the currency rates and currency values of all lines in the specified document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetHardenedDates" type="inx:GetHardenedDatesRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Resolves soft dates set for due date, value date and discount dates in the specified element master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetElmBanks" type="inx:GetElmBanksRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves the specified element's bank details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetElmAddresses" type="inx:GetElmAddressesRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves the specified element's address details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetCustomerSupplierElmSummary" type="inx:GetCustomerSupplierElmSummaryRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Returns summary information for the customer/supplier elements in the specified document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetMissingDocNumbers" type="inx:GetMissingDocNumbersRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves any missing document numbers from the database using the information you provide.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetMissingStatus" type="inx:GetMissingStatusRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Validates whether the specified document number is a missing document number.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Resolve" type="inx:ResolveRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Completes any missing transaction data that can be derived from the account code or from the document value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetUserStatus" type="inx:GetUserStatusRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Derives the user status from the driving element on the specified document line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CopyDoc" type="inx:CopyDocRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Creates a new document by copying an existing document and using the details you specify.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetCopyDocParams" type="inx:GetCopyDocParamsRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retrieves the document date, posting period, and input template master for a document created using the CopyDocRequest.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:all>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

