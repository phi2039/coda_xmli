  <xsd:complexType name="MatchingRequest">
    <xsd:annotation>
      <xsd:documentation>This service allows you to match outstanding debit and credit entries.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ServiceRequest">
        <xsd:all>
          <xsd:element minOccurs="0" name="SelectMaster" type="mat:SelectMasterRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Selects the specified matching master from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ValidateParams" type="mat:ValidateParamsRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Validates the parameters supplied at runtime.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="SelectLines" type="mat:SelectLinesRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Selects debit and credit entries that are available for matching.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetDiscounts" type="mat:GetDiscountRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves discount information for the specified document line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Discount" type="mat:DiscountRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Enters a discount value for the specified document line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PartMatch" type="mat:PartMatchRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Enters a part payment value for the specified document line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="WriteOff" type="mat:WriteOffRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Enters a write-off value for the specified document line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetLineValues" type="mat:GetLineValuesRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves the currency line value information for the specified document line.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetTotals" type="mat:GetTotalsRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves the total values (in matching, home and dual currency) of the selected matching group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetPayDetailsSources" type="mat:GetPayDetailsSourcesRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves the Draft Paid document lines in the selected matching group(s). One Draft Paid document line can be set as the source for data propagation in its matching group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetDrivingLineInfo" type="mat:GetDrivingLineInfoRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves the document lines in the selected matching group(s) where a driving line must be specified. One document line can be set as the driving line in its matching group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Commit" type="mat:CommitRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Commits the match for the selected group. You can do this any time the lines in the selected group sum to zero (that is, when debits and credits balance) in matching, home and dual currencies.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Disperse" type="mat:DisperseRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Performs a disperse match on the selected group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="StartBackground" type="mat:StartBackgroundRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Starts a Background Matching process.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="StartAssistant" type="mat:StartAssistantRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Starts an Assisted Interactive Matching process.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetGroupLines" type="mat:GetGroupLinesRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves information about the matching groups created by the specified Assistant process.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetProgress" type="prg:progressGetRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves information about the number of lines that have been processed by the current Assistant process.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Abort" type="prg:progressAbortRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Stops the current Assistant process. If the process was Assisted Interactive Matching, any matching groups already suggested are retained. If the process was Background Matching, any matching groups already produced are discarded and no matches are committed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GetDocumentChain" type="mat:GetDocumentChainRequestVerb">
            <xsd:annotation>
              <xsd:documentation>Retrieves information about the document chain.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:all>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="MatchingResponse">
    <xsd:annotation>
      <xsd:documentation>This is the response that is returned when you match debit and credit entries.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ServiceResponse">
        <xsd:all>
          <xsd:element minOccurs="0" name="SelectMaster" type="mat:SelectMasterResponseVerb">
            <xsd:annotation>
              <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
