    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ResponseVerb">
        <xsd:sequence>
          <xsd:element name="Response" type="int:GetNewProposalDetailsResponse" minOccurs="0" maxOccurs="unbounded"/>
