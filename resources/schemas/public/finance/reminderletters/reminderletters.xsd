  <xsd:complexType name="LetterDetailList">
    <xsd:annotation>
      <xsd:documentation>This element holds a list of details for the reminder letters in a proposal.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="LetterDetail" type="let:LetterDetail" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>This element holds the details of a reminder letter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

