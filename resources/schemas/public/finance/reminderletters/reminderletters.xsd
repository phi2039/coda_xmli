        <xsd:annotation>
          <xsd:documentation>The code for Element 2.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Element3" type="com:typeElmCode">
        <xsd:annotation>
          <xsd:documentation>The code for Element 3.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Element4" type="com:typeElmCode">
        <xsd:annotation>
          <xsd:documentation>The code for Element 4.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Element5" type="com:typeElmCode">
        <xsd:annotation>
          <xsd:documentation>The code for Element 5.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Element6" type="com:typeElmCode">
        <xsd:annotation>
          <xsd:documentation>The code for Element 6.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Element7" type="com:typeElmCode">
        <xsd:annotation>
          <xsd:documentation>The code for Element 7.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
