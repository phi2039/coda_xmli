        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExternalReference4" type="com:typeExtRefB">
        <xsd:annotation>
          <xsd:documentation>The value of External Reference 4.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
