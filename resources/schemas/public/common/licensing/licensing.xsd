  </xsd:complexType>

  <xsd:complexType name="ResourceLicenceEventKeyList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ResourceLicenceEvent" type="lic:ResourceLicenceEventKey"/>
