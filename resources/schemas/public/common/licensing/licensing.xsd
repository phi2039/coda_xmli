      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ResourceLicenceData">
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Seq" type="com:typeULong"/>
