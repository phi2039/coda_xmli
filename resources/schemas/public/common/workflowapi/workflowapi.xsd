    <xsd:annotation>
      <xsd:documentation>Details of the current workflow process definition diagram.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Zoom" type="com:typeWord">
        <xsd:annotation>
          <xsd:documentation>The zoom level of the workflow process definition diagram.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DiagramList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Diagram" type="wor:Diagram"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="WebServiceActivityDetails">
    <xsd:annotation>
      <xsd:documentation>This element contains the details of a Web service activity.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="ServiceName" type="wsd:QualifiedName">
        <xsd:annotation>
          <xsd:documentation>The name of the Web service.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="PortName" type="com:typeParamName">
        <xsd:annotation>
          <xsd:documentation>The name of the Web service port.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="OperationName" type="com:typeParamName">
        <xsd:annotation>
          <xsd:documentation>The name of the Web service operation.</xsd:documentation>
        </xsd:annotation>
