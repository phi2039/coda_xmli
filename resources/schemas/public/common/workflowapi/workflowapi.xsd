<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:atc="http://www.coda.com/common/schemas/attachment" xmlns:com="http://www.coda.com/efinance/schemas/common" xmlns:datatypes="http://www.coda.com/common/schemas/datatypes" xmlns:gra="http://www.coda.com/common/schemas/graphlayout" xmlns:spec="http://www.coda.com/common/schemas/workflow" xmlns:use="http://www.coda.com/common/schemas/userextension" xmlns:wor="http://www.coda.com/common/schemas/workflowapi" xmlns:wsd="http://www.coda.com/common/schemas/wsdl" elementFormDefault="qualified" targetNamespace="http://www.coda.com/common/schemas/workflowapi">

  <xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/common/types.xsd"/>
  <xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/common/types2.xsd"/>
  <xsd:import namespace="http://www.coda.com/efinance/schemas/common" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/common/common.xsd"/>
  <xsd:import namespace="http://www.coda.com/common/schemas/attachment" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/attachment/attachment.xsd"/>
  <xsd:import namespace="http://www.coda.com/common/schemas/datatypes" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/datatypes/datatypes.xsd"/>
  <xsd:import namespace="http://www.coda.com/common/schemas/graphlayout" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/graphlayout/graphlayout.xsd"/>
  <xsd:import namespace="http://www.coda.com/common/schemas/userextension" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/userextension/userextension.xsd"/>
  <xsd:import namespace="http://www.coda.com/common/schemas/workflow" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/workflow/workflow.xsd"/>
  <xsd:import namespace="http://www.coda.com/common/schemas/wsdl" schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/wsdl/wsdl.xsd"/>

  <xsd:complexType name="WebServiceParameterMapping">
    <xsd:annotation>
      <xsd:documentation>Details of the mapping between a Web service parameter and a workflow variable.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="ParameterName" type="com:typeParamName">
        <xsd:annotation>
          <xsd:documentation>The Web service parameter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="ParameterKind" type="com:typeComParameterKind">
        <xsd:annotation>
          <xsd:documentation>The type of parameter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="VariableName" type="com:typeName">
        <xsd:annotation>
          <xsd:documentation>A variable name for the parameter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="Literal" type="com:typeTextAnyUnlimitedQuotes">
        <xsd:annotation>
          <xsd:documentation>A literal text string for the parameter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="WebServiceParameterMappingList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WebServiceParameterMapping" type="wor:WebServiceParameterMapping">
        <xsd:annotation>
          <xsd:documentation>Details of the mapping between a Web service parameter and a workflow variable.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="CompletedResponseData">
    <xsd:annotation>
      <xsd:documentation>The returned workflow work item data.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Name" type="com:typeName">
        <xsd:annotation>
          <xsd:documentation>The name of a data item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="Value" type="com:typeTextAny">
        <xsd:annotation>
          <xsd:documentation>The value associated with the label.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="CompletedResponseDataList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="CompletedResponseData" type="wor:CompletedResponseData">
        <xsd:annotation>
          <xsd:documentation>The returned workflow work item data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="LiteralURLSource">
    <xsd:complexContent>
      <xsd:extension base="wor:URLSource">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" name="Url" type="com:typeUri"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="LiteralURLSourceList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LiteralURLSource" type="wor:LiteralURLSource"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ImportInformation">
    <xsd:annotation>
      <xsd:documentation>This element contains the information returned by the WorkflowQuery call.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="EffectiveDate" type="com:typeHard">
        <xsd:annotation>
          <xsd:documentation>The date the workflow process definition becomes effective.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="QueryDate" type="com:typeHard">
        <xsd:annotation>
          <xsd:documentation>The date the query was executed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="Valid" type="com:typeBoolean">
        <xsd:annotation>
          <xsd:documentation>If set, indicates that the workflow process definition is valid.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="NewTypeScope" type="com:typeBoolean">
        <xsd:annotation>
          <xsd:documentation>If set, indicates prescence of a new Type Scope.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="TypeScopeCode" type="com:typeCode">
        <xsd:annotation>
          <xsd:documentation>The code for the Type Scope.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="TypeScopeVersion" type="com:typeLong">
        <xsd:annotation>
          <xsd:documentation>The version of the Type Scope.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="TypesFoundInData" type="com:typeBoolean">
        <xsd:annotation>
          <xsd:documentation>If set, indicates that Type Scopes have been found in the data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ImportMessageList" type="wor:ImportInformationImportMessageList">
        <xsd:annotation>
          <xsd:documentation>A list of import messages.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ImportInformationImportMessageList">
    <xsd:annotation>
      <xsd:documentation>A list of import messages.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ImportMessage" type="wor:ImportMessage">
        <xsd:annotation>
          <xsd:documentation>An import message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ImportInformationList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ImportInformation" type="wor:ImportInformation"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="NewVersionMessage">
    <xsd:complexContent>
      <xsd:extension base="wor:ProcessDefinitionMessage">
        <xsd:sequence>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="NewVersionMessageList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NewVersionMessage" type="wor:NewVersionMessage"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ProcessDefinitionItem">
    <xsd:annotation>
      <xsd:documentation>The details of a workflow process definition item.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Code" type="com:typeCode">
        <xsd:annotation>
          <xsd:documentation>The workflow process definition code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="Version" type="com:typeLong">
        <xsd:annotation>
          <xsd:documentation>The workflow process definition version number.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="Name" type="com:typeName">
        <xsd:annotation>
          <xsd:documentation>The item name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="ModifiedDate" type="com:typeHard">
        <xsd:annotation>
          <xsd:documentation>The date the item was modified.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="EffectiveDate" type="com:typeHard">
        <xsd:annotation>
          <xsd:documentation>The date the item becomes effective.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="ShortName" type="com:typeShortNameB">
        <xsd:annotation>
          <xsd:documentation>The item short name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="CodaProvided" type="com:typeBoolean">
        <xsd:annotation>
          <xsd:documentation>Specifies whether this process definition master is provided by CODA. This field is read-only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ProcessDefinitionItemList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ProcessDefinitionItem" type="wor:ProcessDefinitionItem">
        <xsd:annotation>
          <xsd:documentation>The details of a workflow process definition item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ManualActivityDetails">
    <xsd:annotation>
      <xsd:documentation>This element contains the details of a manual activity.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Description" type="com:typeTextAny">
        <xsd:annotation>
          <xsd:documentation>A description of the activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="AssignTo" type="com:typeText">
        <xsd:annotation>
          <xsd:documentation>The user, position or variable to assign the activity to.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="AssignToType" type="com:typeComWorkflowAssign">
        <xsd:annotation>
          <xsd:documentation>Identifies the type of the value specified in AssignTo.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DescriptionParameterList" type="wor:ManualActivityDetailsDescriptionParameterList">
        <xsd:annotation>
          <xsd:documentation>A list of parameters used in the description text.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="TimeoutExpression" type="com:typeTextAnyUnlimitedQuotes">
        <xsd:annotation>
          <xsd:documentation>A script determining the action to take in a time-out condition.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ManualActivityResponseList" type="wor:ManualActivityDetailsManualActivityResponseList">
        <xsd:annotation>
          <xsd:documentation>A list of valid responses for the activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ManualActivityFieldList" type="wor:ManualActivityDetailsManualActivityFieldList">
        <xsd:annotation>
          <xsd:documentation>A list of additional fields which will appear when progressing the work item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="ExplodeURLOverride" type="wor:URLSource">
        <xsd:annotation>
          <xsd:documentation>A URL which overrides the one specified for the workflow process definition.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ManualActivityDetailsDescriptionParameterList">
    <xsd:annotation>
      <xsd:documentation>A list of parameters used in the description text.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DescriptionParameter" type="com:typeName">
        <xsd:annotation>
          <xsd:documentation>The name of the variable used as a parameter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ManualActivityDetailsManualActivityResponseList">
    <xsd:annotation>
      <xsd:documentation>A list of valid responses for the activity.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="ManualActivityResponse" type="wor:ManualActivityResponse">
        <xsd:annotation>
          <xsd:documentation>Details of a valid response for the activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ManualActivityDetailsManualActivityFieldList">
    <xsd:annotation>
      <xsd:documentation>A list of additional fields which will appear when progressing the work item.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ManualActivityField" type="wor:ManualActivityField">
        <xsd:annotation>
          <xsd:documentation>Details of an additional field.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ManualActivityDetailsList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ManualActivityDetails" type="wor:ManualActivityDetails"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Option">
    <xsd:annotation>
      <xsd:documentation>The option details.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Label" type="com:typeText">
        <xsd:annotation>
          <xsd:documentation>The label for the option.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AvailableOptionDataList" type="wor:OptionAvailableOptionDataList">
        <xsd:annotation>
          <xsd:documentation>A list of available data for the option.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MandatoryOptionDataList" type="wor:OptionMandatoryOptionDataList">
        <xsd:annotation>
          <xsd:documentation>A list of mandatory data for the option.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="OptionAvailableOptionDataList">
    <xsd:annotation>
      <xsd:documentation>A list of available data for the option.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AvailableOptionData" type="com:typeName">
        <xsd:annotation>
          <xsd:documentation>The data for the option.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="OptionMandatoryOptionDataList">
    <xsd:annotation>
      <xsd:documentation>A list of mandatory data for the option.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="MandatoryOptionData" type="com:typeName">
        <xsd:annotation>
          <xsd:documentation>The data for the option.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="OptionList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Option" type="wor:Option">
        <xsd:annotation>
          <xsd:documentation>The option details.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ImportMessage">
    <xsd:annotation>
      <xsd:documentation>An import message.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ImportMessageList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ImportMessage" type="wor:ImportMessage">
        <xsd:annotation>
          <xsd:documentation>An import message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SubWorkflowActivityDetails">
    <xsd:annotation>
      <xsd:documentation>This element contains the details of a sub-workflow activity.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Code" type="com:typeCode">
        <xsd:annotation>
          <xsd:documentation>The code for the sub-workflow activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1" name="Version" type="com:typeLong">
        <xsd:annotation>
          <xsd:documentation>The version of the sub-workflow activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MappingList" type="wor:SubWorkflowActivityDetailsMappingList">
        <xsd:annotation>
          <xsd:documentation>This element contains a list of mappings between the current workflow process definition variables and the sub-workflow process definition variables.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SubWorkflowActivityDetailsMappingList">
    <xsd:annotation>
      <xsd:documentation>This element contains a list of mappings between the current workflow process definition variables and the sub-workflow process definition variables.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Mapping" type="wor:SubWorkflowVariableMapping">
        <xsd:annotation>
          <xsd:documentation>Details of a mapping between variables.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SubWorkflowActivityDetailsList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="SubWorkflowActivityDetails" type="wor:SubWorkflowActivityDetails"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Descendents">
    <xsd:annotation>
      <xsd:documentation>This element contains lists of subworkflow IDs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="IdList" type="wor:DescendentsIdList">
        <xsd:annotation>
          <xsd:documentation>This element holds a list of IDs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DescendentsIdList">
    <xsd:annotation>
      <xsd:documentation>This element holds a list of IDs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Id" type="com:typeDbGuid">
        <xsd:annotation>
          <xsd:documentation>A subworkflow ID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DescendentsList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Descendents" type="wor:Descendents"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MissingTypeMessage">
    <xsd:complexContent>
      <xsd:extension base="wor:ImportMessage">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" name="DataType" type="wsd:QualifiedName"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="MissingTypeMessageList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="MissingTypeMessage" type="wor:MissingTypeMessage"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="WorkflowStartInformation">
    <xsd:annotation>
      <xsd:documentation>This element contains details of the initial values of a workflow instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Code" type="com:typeCode">
        <xsd:annotation>
          <xsd:documentation>The workflow process definition code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="Priority" type="com:typeComWorkflowPriority">
        <xsd:annotation>
          <xsd:documentation>The instance priority.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="ExplodeURL" type="com:typeTextAnyDbQuotes">
        <xsd:annotation>
          <xsd:documentation>The URL to display when exploding a work item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AttachmentIdList" type="wor:WorkflowStartInformationAttachmentIdList"/>
      <xsd:element maxOccurs="1" minOccurs="0" name="PrimaryAttachmentId" type="com:typeDbGuid"/>
      <xsd:element name="WorkflowParameterList" type="wor:WorkflowStartInformationWorkflowParameterList">
        <xsd:annotation>
          <xsd:documentation>This element contains a list of workflow parameters.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="WorkflowStartInformationAttachmentIdList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AttachmentId" type="com:typeDbGuid"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="WorkflowStartInformationWorkflowParameterList">
    <xsd:annotation>
      <xsd:documentation>This element contains a list of workflow parameters.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WorkflowParameter" type="wor:WorkflowParameter">
        <xsd:annotation>
          <xsd:documentation>This element contains details of a workflow parameter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="WorkflowStartInformationList">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WorkflowStartInformation" type="wor:WorkflowStartInformation"/>
    </xsd:sequence>
  </xsd:complexType>

