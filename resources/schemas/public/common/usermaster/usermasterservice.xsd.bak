            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UpdateHomePage" type="usr:UpdateHomePageRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="4" build:readOnly="false"/>
              <xsd:documentation>Assigns the specified Homepage template to the specified CODA user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Get" type="usr:GetRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="5" build:readOnly="true"/>
              <xsd:documentation>Retrieves the specified CODA user masters from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Delete" type="usr:DeleteRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="6" build:readOnly="false"/>
              <xsd:documentation>Deletes the specified CODA user masters from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="List" type="usr:ListRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="7" build:readOnly="true"/>
              <xsd:documentation>Retrieves a list of keys for the specified CODA user masters from the database.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Export" type="usr:ExportRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="8" build:readOnly="true"/>
              <xsd:documentation>Exports all CODA user masters on the system to an XML file.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Import" type="usr:ImportRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="9" build:readOnly="false"/>
              <xsd:documentation>Imports data from an XML file to create new CODA user masters or update existing ones.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ListForApplication" type="usr:ListForApplicationRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="10" build:readOnly="true"/>
              <xsd:documentation>Retrieves a list of CODA user masters that are set up to use the specified application.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="TurnOffOutOfOffice" type="usr:TurnOffOutOfOfficeRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="11" build:readOnly="false"/>
              <xsd:documentation>Turns off the out of office flag for the specified user.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetOutOfOffice" type="usr:GetOutOfOfficeRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="12" build:readOnly="true"/>
              <xsd:documentation>Retrieves out of office details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GetWithPositions" type="usr:GetWithPositionsRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="13" build:readOnly="true"/>
              <xsd:documentation>Retrieves the specified CODA user masters from the database, together with the information needed to manage users' position associations. For each user, this information is the list of positions the user is currently associated with and the set of position hierarchies that contain them.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AddWithPositions" type="usr:AddWithPositionsRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="14" build:readOnly="false"/>
              <xsd:documentation>Creates new CODA user masters in the database using the information you provide, and associates each user with the positions in the supplied list.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UpdateWithPositions" type="usr:UpdateWithPositionsRequestVerb" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo build:funcSecValue="15" build:readOnly="false"/>
              <xsd:documentation>Updates CODA user masters and their associations with position masters in the database using the information you provide.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:all>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

