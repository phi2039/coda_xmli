  <xsd:complexType name="GetWithPositionsRequest">
    <xsd:complexContent>
      <xsd:extension base="usr:GetRequest">
        <xsd:sequence>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="GetWithPositionsRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Retrieves the specified CODA user masters from the database, together with the information needed to manage users' position associations. For each user, this information is the list of positions the user is currently associated with and the set of position hierarchies that contain them.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="usr:GetWithPositionsRequest" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="GetWithPositionsResponse">
    <xsd:complexContent>
      <xsd:extension base="usr:GetResponse">
        <xsd:sequence>
          <xsd:element name="SelectedPositions" type="usr:Positions">
            <xsd:annotation>
              <xsd:documentation>The list of positions with which this user is associated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SelectedHierarchies" type="usr:Hierarchies">
            <xsd:annotation>
              <xsd:documentation>The list of position hierarchies in which the positions associated with this user occur.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="GetWithPositionsResponseVerb">
    <xsd:annotation>
      <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ResponseVerb">
        <xsd:sequence>
          <xsd:element name="Response" type="usr:GetWithPositionsResponse" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AddWithPositionsRequest">
    <xsd:complexContent>
      <xsd:extension base="usr:AddRequest">
        <xsd:sequence>
          <xsd:element name="Positions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions that this new user is associated with.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AddWithPositionsRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Creates new CODA user masters in the database using the information you provide, and associates each user with the positions in the supplied list.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="usr:AddWithPositionsRequest" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AddWithPositionsResponse">
    <xsd:complexContent>
      <xsd:extension base="usr:AddResponse">
        <xsd:sequence>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AddWithPositionsResponseVerb">
    <xsd:annotation>
      <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ResponseVerb">
        <xsd:sequence>
          <xsd:element name="Response" type="usr:AddWithPositionsResponse" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="UpdateWithPositionsRequest">
    <xsd:complexContent>
      <xsd:extension base="usr:UpdateRequest">
        <xsd:sequence>
          <xsd:element name="AddPositions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions that you want to add this user to.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="RemovePositions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions that you want to remove this user from.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CheckPositionsLeftConnected" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions that this user was associated with when you retrieved the user master from the database, and from which you have not removed this user. If either the user has been removed from any of these positions or associated with any other positions, the response document warns you of this and provides lists of externally added, removed and deleted positions. This allows you to make any changes and attempt another update.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="UpdateWithPositionsRequestVerb">
    <xsd:annotation>
      <xsd:documentation>Updates CODA user masters and their associations with position masters in the database using the information you provide.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:RequestVerb">
        <xsd:sequence>
          <xsd:element name="Request" type="usr:UpdateWithPositionsRequest" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="UpdateWithPositionsResponse">
    <xsd:complexContent>
      <xsd:extension base="usr:UpdateResponse">
        <xsd:sequence>
          <xsd:element name="Success" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Specifies whether the update was successful (true) or the association of the user with positions was changed by someone else while you were editing the user master (false).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ExternallyAddedPositions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions with which the user has been associated by someone else while you were editing the user master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ExternallyRemovedPositions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions from which the user has been removed by someone else while you were editing the user master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ExternallyDeletedPositions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of position masters that have been deleted by someone else while you were editing the user master.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SelectedPositions" type="usr:Positions" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of positions with which the user is now associated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SelectedHierarchies" type="usr:Hierarchies" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of position hierarchies in which the positions now associated with this user now occur.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="UpdateWithPositionsResponseVerb">
    <xsd:annotation>
      <xsd:documentation>Contains the responses to your requests with this verb.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:ResponseVerb">
        <xsd:sequence>
          <xsd:element name="Response" type="usr:UpdateWithPositionsResponse" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ListRequest">
    <xsd:complexContent>
      <xsd:extension base="com:Request">
        <xsd:sequence>
          <xsd:element name="Filter" type="usr:ListFilter">
            <xsd:annotation>
