<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:com="http://www.coda.com/efinance/schemas/common" targetNamespace="http://www.coda.com/efinance/schemas/common" elementFormDefault="qualified">

  <xsd:include schemaLocation="http://cda-dev.aws3.liiaws.net/xmli-schemas/public/common/common/languages.xsd"/>

  <xsd:simpleType name="typeBaseTime">
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBaseSoftDate">
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBaseYearPeriod">
    <xsd:annotation>
      <xsd:documentation>The basic type of the year and period field. For hard year and period combinations this has the format of a four-digit year, a forward slash, and a period. For example, '2001/2'.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBaseYear">
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBaseEnum">
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBaseEnumList">
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBaseByte">
    <xsd:restriction base="xsd:short">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeUByte">
    <xsd:restriction base="com:typeBaseByte">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeByte">
    <xsd:restriction base="com:typeBaseByte">
      <xsd:minInclusive value="-128"/>
      <xsd:maxInclusive value="127"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeUWord">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeUWord2">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeUWordB">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeWord">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="-32768"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeWordB">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="-32768"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeWordZeroB">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="-32768"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeULong">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="2147483647"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeULongB">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="2147483647"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeLong">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="-2147483648"/>
      <xsd:maxInclusive value="2147483647"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeLongB">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="-2147483648"/>
      <xsd:maxInclusive value="2147483647"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeStatIntB">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="2147483647"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeLongYE">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1000"/>
      <xsd:maxInclusive value="99999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeNum">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="-99999999999999999"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeNumB">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="-99999999999999999"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeMoney">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="-99999999999999999"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeMoney2B">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="-99999999999999999"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeMoneyB">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="-99999999999999999"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeMoneyAuto">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="-99999999999999999"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePercent">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="100"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeBigPercent">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeRate">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeRateB">
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeDecPlace">
    <xsd:annotation>
      <xsd:documentation>The number of decimal places.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="9"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeSingleDigit">
    <xsd:annotation>
      <xsd:documentation>A single digit between 0 and 9.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="9"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeSingleDigit1To9">
    <xsd:annotation>
      <xsd:documentation>A single digit between 1 and 9.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="9"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeMatchingOffsetDays">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeRTSProtectionOffset">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeElmLevel">
    <xsd:annotation>
      <xsd:documentation>The element level.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="8"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeElmLevelB">
    <xsd:annotation>
      <xsd:documentation>The element level.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="8"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeRepBasis">
    <xsd:annotation>
      <xsd:documentation>A number in the range 1-255 which indicates the account intersects selected for maintaining balances. The number 0 represents a full account code. See the Glossary in Help for more information.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeQtyPosition">
    <xsd:annotation>
      <xsd:documentation>The position of the quantity (1 - 4) held for an element.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="4"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeAccumLimit">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePauseRequestSize">
    <xsd:annotation>
      <xsd:documentation>The number of documents that Archive will select before the server checks to see if a pause or stop request has been issued.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1000"/>
      <xsd:maxInclusive value="999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeClusterSize">
    <xsd:annotation>
      <xsd:documentation>The number of documents processed into a single archive journal.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="10"/>
      <xsd:maxInclusive value="999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeRecPercent">
    <xsd:annotation>
      <xsd:documentation>The percentage of tax that is recoverable.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="100"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTaxRate">
    <xsd:annotation>
      <xsd:documentation>The tax rate.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="99999999999999999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePeriods">
    <xsd:annotation>
      <xsd:documentation>The number of financials periods in the company's year, between 1 and 9999.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="9999"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePeriodRangeChk">
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="9997"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeSeverity">
    <xsd:annotation>
      <xsd:documentation>The severity level of the reminder letter, between 1 and 9.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="9"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeScaling">
    <xsd:annotation>
      <xsd:documentation>The scaling rate to be applied to currency values.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="1000000"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeGrpLevel">
    <xsd:annotation>
      <xsd:documentation>The level at which a group exists. For element groups this is 1-8. For a company grup, it is 0.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="-32768"/>
      <xsd:maxInclusive value="8"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePartPaidOrig">
    <xsd:annotation>
      <xsd:documentation>The type of line resulting from a part payment. It can be one of the following values:</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:short">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="4"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeText">
    <xsd:annotation>
      <xsd:documentation>A text string.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextB">
    <xsd:annotation>
      <xsd:documentation>A text string. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextUpper">
    <xsd:annotation>
      <xsd:documentation>An uppercase text string.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextUpperB">
    <xsd:annotation>
      <xsd:documentation>An uppercase text string. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextWild">
    <xsd:annotation>
      <xsd:documentation>A text string. This type can include wildcards or be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextUpperWild">
    <xsd:annotation>
      <xsd:documentation>An uppercase text string. This type can include wildcards.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextUpperWildB">
    <xsd:annotation>
      <xsd:documentation>An uppercase text string. This type can include wildcards or be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAny">
    <xsd:annotation>
      <xsd:documentation>A text string.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAnyB">
    <xsd:annotation>
      <xsd:documentation>A text string. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAnyUpper">
    <xsd:annotation>
      <xsd:documentation>An uppercase text string.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAnyUpperB">
    <xsd:annotation>
      <xsd:documentation>An uppercase text string. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAlphaNumeric">
    <xsd:annotation>
      <xsd:documentation>An alphanumeric text string.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAlphaNumericB">
    <xsd:annotation>
      <xsd:documentation>An alphanumeric text string. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTextAlphaNumericUnderScB">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeUpper">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeStatUser">
    <xsd:annotation>
      <xsd:documentation>A single character user identifier.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeStatUserB">
    <xsd:annotation>
      <xsd:documentation>A single character user identifier. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePassword">
    <xsd:annotation>
      <xsd:documentation>A user password.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="12"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeExtRef">
    <xsd:annotation>
      <xsd:documentation>An external reference.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="32"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeExtRefB">
    <xsd:annotation>
      <xsd:documentation>An external reference. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="32"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeExtRefWild">
    <xsd:annotation>
      <xsd:documentation>An external reference. This type can include wildcards.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="32"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeExtRefWildB">
    <xsd:annotation>
      <xsd:documentation>An external reference. This type can include wildcards or be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="32"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePayUserRef">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="35"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeDescr">
    <xsd:annotation>
      <xsd:documentation>A text description.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="36"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeCode">
    <xsd:annotation>
      <xsd:documentation>A Financials code. The allowed length depends on the type of record the code is for.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeCodeB">
    <xsd:annotation>
      <xsd:documentation>A Financials code. The allowed length depends on the type of record the code is for. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeCodeWild">
    <xsd:annotation>
      <xsd:documentation>A Financials code. The allowed length depends on the type of record the code is for. This type allows wildcards.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePrlCode">
    <xsd:annotation>
      <xsd:documentation>A proposal code.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="28"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typePrlCodeB">
    <xsd:annotation>
      <xsd:documentation>A proposal code. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="28"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTraderCode">
    <xsd:annotation>
      <xsd:documentation>A trader code.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="36"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTraderCodeB">
    <xsd:annotation>
      <xsd:documentation>A trader code. This type can be blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="36"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTraderCodeWild">
    <xsd:annotation>
      <xsd:documentation>A trader code. This type allows wildcards.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="36"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeTraderCodeWildB">
    <xsd:annotation>
      <xsd:documentation>A trader code. This type allows wildcards or a blank.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="36"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeGeneralCode">
    <xsd:annotation>
      <xsd:documentation>This is the basic type of codes in Financials. This type does not allow blank codes.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="12"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeGeneralCodeB">
    <xsd:annotation>
      <xsd:documentation>This type for codes allows blank codes, but otherwise is the same as the basic type of codes in Financials (typeGeneralCode).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="12"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeElmCode">
    <xsd:annotation>
      <xsd:documentation>This is the type of element codes. This type does not allow blank codes or wildcards.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="72"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeElmUpperAndLowerCaseCodeWildB">
    <xsd:annotation>
      <xsd:documentation>This is the type of element codes. This type allows upper and lower case characters, blank codes or wildcards.</xsd:documentation>
    </xsd:annotation>
