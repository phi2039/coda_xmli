  <xsd:simpleType name="typeCtWorkflowResponsibilityOperator">
    <xsd:annotation>
      <xsd:documentation>The operator used in the selection criteria for submitting Workflow documents for authorisation.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="com:typeBaseEnum">
      <xsd:enumeration value="op_eq">
        <xsd:annotation>
          <xsd:documentation>= (equal to) 7 8 9 (In range) 10</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_ne">
        <xsd:annotation>
          <xsd:documentation>&lt;&gt; (not equal to)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_lt">
        <xsd:annotation>
          <xsd:documentation>&lt; (less than)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_gt">
        <xsd:annotation>
          <xsd:documentation>&gt; (greater than)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_lte">
        <xsd:annotation>
          <xsd:documentation>&lt;= (less than or equal to)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_gte">
        <xsd:annotation>
          <xsd:documentation>&gt;= (greater than or equal to)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_like">
        <xsd:annotation>
          <xsd:documentation>LIKE (matches wildcard string)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_unlike">
        <xsd:annotation>
          <xsd:documentation>NOTLIKE (does not match wildcard string)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_range">
        <xsd:annotation>
          <xsd:documentation>RANGE (in the specified range)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="op_nrange">
        <xsd:annotation>
          <xsd:documentation>NRANGE (not in the specified range)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="Money">
    <xsd:annotation>
