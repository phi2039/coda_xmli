    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:decimal">
        <xsd:attribute name="companycode" type="com:typeCode" use="optional">
          <xsd:annotation>
            <xsd:documentation>A company code.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="currencycode" type="com:typeCode" use="optional">
          <xsd:annotation>
            <xsd:documentation>A currency code.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Criteria">
    <xsd:annotation>
      <xsd:documentation>The details of a query.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Operator" type="com:typeQueryOpList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An operator for the query.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An item name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Value" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The value for the item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="CompoundCriteria">
    <xsd:complexContent>
      <xsd:extension base="com:Criteria">
        <xsd:sequence>
          <xsd:element name="Conjunction" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>A logical operator.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Criteria" type="com:Criteria" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The details of a query.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Query">
    <xsd:annotation>
      <xsd:documentation>This element holds details of a query.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="com:CompoundCriteria">
        <xsd:sequence>
          <xsd:element name="RowLimit" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation>The maximum number of rows to retrieve.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ShortcutParameter">
    <xsd:annotation>
      <xsd:documentation>The shortcut parameters.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The name of the shortcut.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="value" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The value of the shortcut.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Shortcut">
    <xsd:annotation>
      <xsd:documentation>A shortcut command.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ShortcutParameters" type="com:ShortcutParameter" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The shortcut parameters.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MenuItem">
    <xsd:annotation>
      <xsd:documentation>The details of a menu item.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Text" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The text for the menu item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="URL" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The URL for the menu item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Application" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The CODA application.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Shortcut" type="com:Shortcut">
        <xsd:annotation>
          <xsd:documentation>A shortcut command.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MenuItems" type="com:MenuItems">
        <xsd:annotation>
          <xsd:documentation>The details of menu items.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OpenInNewWindow" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>If TRUE, opens the target in a new window.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MenuItems">
    <xsd:annotation>
      <xsd:documentation>This element holds details of menu items.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="MenuItem" type="com:MenuItem" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The details of a menu item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>


</xsd:schema>
