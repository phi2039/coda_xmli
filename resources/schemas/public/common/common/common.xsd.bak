  <xsd:simpleType name="typeCtWorkflowSecurityLevel">
    <xsd:annotation>
      <xsd:documentation>The type of access to workflow functionality.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="com:typeBaseEnum">
      <xsd:enumeration value="com_wf_sec_level_participant">
        <xsd:annotation>
          <xsd:documentation>Participant users may send and receive workflow e-mail messages.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="com_wf_sec_level_designer">
        <xsd:annotation>
          <xsd:documentation>Designer users may create and modify workflow designs.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="com_wf_sec_level_administrator">
        <xsd:annotation>
          <xsd:documentation>Administrator users may view workflow designs, cancel workflows, re-route workflows and adjust the workflow scheduling processes.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="com_wf_sec_level_super">
        <xsd:annotation>
          <xsd:documentation>Super users may perform any workflow activity.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="typeCtMessageFrequencyScale">
